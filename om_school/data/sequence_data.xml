<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Sequences for student.info -->
    <record id="seq_student_info" model="ir.sequence">
        <field name="name">Student Info</field>
        <field name="code">student.info</field>
        <field name="prefix">STU</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>


    <!-- Sequences for school.applicationregister (Application Sequence -->
    <record id="seq_school_application_register" model="ir.sequence">
        <field name="name">School Application Register</field>
        <field name="code">school.applicationregister</field>
        <field name="prefix">AP</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Sequences for teacher.info -->
    <record id="seq_admin_info" model="ir.sequence">
        <field name="name">Administration Info</field>
        <field name="code">administration.info</field>
        <field name="prefix"></field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>